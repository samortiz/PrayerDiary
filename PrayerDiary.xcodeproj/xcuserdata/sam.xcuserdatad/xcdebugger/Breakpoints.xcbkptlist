<?xml version="1.0" encoding="UTF-8"?>
<Bucket
   type = "1"
   version = "1.0">
   <ExceptionBreakpoints>
      <ExceptionBreakpoint
         shouldBeEnabled = "Yes"
         ignoreCount = "0"
         continueAfterRunningActions = "No"
         scope = "1"
         stopOnStyle = "0">
         <Actions>
            <BreakpointActionProxy
               ActionType = "Xcode.BreakpointAction.DebuggerCommand">
               <ActionContent
                  consoleCommand = "bt">
               </ActionContent>
            </BreakpointActionProxy>
            <BreakpointActionProxy
               ActionType = "Xcode.BreakpointAction.DebuggerCommand">
               <ActionContent
                  consoleCommand = "po $eax ">
               </ActionContent>
            </BreakpointActionProxy>
         </Actions>
      </ExceptionBreakpoint>
   </ExceptionBreakpoints>
</Bucket>
